<server description="Servlet server">
	<featureManager>
		<feature>servlet-4.0</feature>
	</featureManager>
	<httpEndpoint httpPort="${default.http.port}" httpsPort="${default.https.port}" id='defaultHttpEndpoint' host="*" />
	<webApplication id="${serverName}" location="${serverName}.war" contextRoot='/' />
</server>
